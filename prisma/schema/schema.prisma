datasource db {
    provider = "postgres"
    url      = env("DATABASE_URL")
}

generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["prismaSchemaFolder"]
}

generator json {
    provider = "prisma-json-types-generator"
}

enum BillingType {
    PER_GUEST_DAILY
    PER_GUEST
    DAILY
    PER_RESERVATION
    PER_HOUSING_UNIT
}
