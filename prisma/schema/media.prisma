model Media {
    id  String @id @default(uuid())
    url String

    /// [MediaMetadata]
    metadata Json

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    housingUnitTypeMedias HousingUnitTypeMedia[]
    serviceMedias         ServiceMedia[]
    bannerMedias          BannerMedia[]

    @@map("medias")
}
